{
  "builds": [
    {
      "src": "fitporx/asgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "3.12",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/public/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "fitporx/asgi.py"
    }
  ]
}
