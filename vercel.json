{
    "builds":[
        {
            "src": "fitporx/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "3.13"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "fitporx/wsgi.py"
        }
    ]
}